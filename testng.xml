<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Flipkart suite">
<listeners>
		<listener class-name="com.flipkart.qa.base.ExtentReportListener" />
	</listeners>
	<parameter name="browserType" value="chrome" />
	<parameter name="appURL" value="https://google.com"/>
  <test  name="Flipkart Test">
    <classes>
      <class name="com.flipkart.qa.testcases.FlipkartLoginPageTest"/>
      <class name="com.flipkart.qa.testcases.FlipkartHomePageTest"/>
      <class name="com.flipkart.qa.testcases.FlipkartItemDetailsPageTest"/>
      <class name="com.flipkart.qa.testcases.FlipkartCartPageTest"/>
      <class name="com.flipkart.qa.testcases.FlipkartCheckOutPageTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
